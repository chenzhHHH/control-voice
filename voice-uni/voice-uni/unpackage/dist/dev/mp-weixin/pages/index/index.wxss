.search-container {
  background-color: #54bf8e;
}
.swiper-container {
  padding: 10rpx 0 30rpx 0;
  background-color: #54bf8e;
}
.swiper-container .swiper {
  width: 600rpx;
  height: 400rpx;
  text-align: center;
  margin: auto;
}
.swiper-container .swiper .swiper-image {
  width: 600rpx;
  height: 400rpx;
}
.container .container-item {
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: center;
  padding: 20rpx 0;
}
.container .container-item .item-box {
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: center;
  width: 200rpx;
  height: 200rpx;
  border: 1rpx solid #c0c0c0;
  border-radius: 30rpx;
}
.container .container-item .item-box .item-text {
  margin-top: 10rpx;
}
