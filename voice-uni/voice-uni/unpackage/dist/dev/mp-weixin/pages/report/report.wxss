.title-container {
  padding: 60rpx 0 60rpx 0;
}
.title-container .cn-title-container {
  text-align: center;
  font-size: 50rpx;
  font-weight: 400;
}
.title-container .en-title-container {
  text-align: center;
  font-size: 32rpx;
  font-weight: 200;
}
.report-result-container {
  width: 580rpx;
  height: 726rpx;
  margin: 90rpx auto 0 auto;
  border-radius: 66rpx;
  background-color: #F2F4F4;
  position: relative;
}
.report-result-container .logo-container {
  width: 174rpx;
  height: 174rpx;
  margin: auto;
  position: absolute;
  top: -110rpx;
  left: 0;
  right: 0;
}
.report-result-container .logo-container .logo-box {
  width: 174rpx;
  height: 174rpx;
}
.report-result-container .result-container {
  width: 464rpx;
  position: absolute;
  top: 116rpx;
  left: 58rpx;
}
.report-result-container .result-container .total-container {
  width: 464rpx;
  height: 250rpx;
  border-radius: 44rpx;
  background-color: #A9DFBF;
}
.report-result-container .result-container .total-container .total-text-box {
  position: absolute;
  top: 44rpx;
  left: 46rpx;
  font-size: 30rpx;
  color: #145A32;
  font-weight: 350;
}
.report-result-container .result-container .total-container .total-score-box {
  position: absolute;
  top: 102rpx;
  left: 46rpx;
  font-size: 90rpx;
  color: #145A32;
  font-weight: 500;
}
.report-result-container .result-container .result-detail-container {
  margin-top: 30rpx;
}
.report-result-container .result-container .result-detail-container .detail-box {
  padding: 15rpx 0 15rpx 30rpx;
}
.report-result-container .result-container .result-detail-container .detail-box .detail-text-box {
  font-size: 32rpx;
  font-weight: 200;
}
.space-container {
  height: 200rpx;
}
.button-container {
  position: fixed;
  left: 50%;
  bottom: 40rpx;
  border-top: 2rpx;
  transform: translate(-50%, 0);
  display: flex;
}
.button-container .retry-button-box {
  width: 300rpx;
  border-radius: 50rpx;
  background-color: #26ac28;
  color: white;
  font-size: 30rpx;
  margin-right: 40rpx;
}
.button-container .next-button-box {
  width: 300rpx;
  border-radius: 50rpx;
  background-color: #26ac28;
  color: white;
  font-size: 30rpx;
}
